{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
      "./env.d.ts", "src/**/*.ts", "src/**/*.vue", "./components.d.ts", "src/util/filter"],
    "exclude": ["./src/**/__tests__/*"],
    "compilerOptions": {
      "composite": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
      },
      "types": ["node"]
    }
  }